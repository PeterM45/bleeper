{
  "name": "bleeper",
  "version": "0.1.3",
  "description": "Ultra-lightweight, zero-dependency profanity filter",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node --test tests/filter.test.js tests/contains.test.js tests/analyze.test.js scripts/perf-test.js",
    "clean": "rm -rf dist",
    "check-version": "node scripts/check-version.js",
    "bump": "node scripts/bump.js",
    "setup-hooks": "git config core.hooksPath .githooks",
    "prepack": "npm run clean && npm run build && npm run check-version"
  },
  "keywords": [
    "profanity",
    "filter",
    "typescript",
    "lightweight",
    "zero-dependencies"
  ],
  "author": "PeterM45",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeterM45/bleeper.git"
  },
  "bugs": {
    "url": "https://github.com/PeterM45/bleeper/issues"
  },
  "homepage": "https://github.com/PeterM45/bleeper#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}
